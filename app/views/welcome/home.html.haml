    %h5
      Total Paid Commission By Lab:
      = number_to_currency(current_user.total_paid_doctors_commission_by_lab, :unit => "Rs:")
.row
  .span6
    %h5
      Total Paid Commission:
      = number_to_currency(current_user.total_paid_doctors_commission, :unit => "Rs:")
