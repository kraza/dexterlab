      Active Test Category:
      = @total_active_test_category
  .span8.textAlignCenter
    %h5
      Inactive Test Category:
      = @total_inactive_test_category
.row
  .span8.textAlignCenter
    %h5
      Active Test:
      = @total_active_test
  .span8.textAlignCenter
    %h5
      Inactive Test:
      = @total_inactive_test
.row
  .span8.textAlignCenter
    %h5
      Active Doctor:
      = @total_active_doctor
  .span8.textAlignCenter
    %h5
      Inactive Doctor:
      = @total_inactive_doctor
.row
  .span8.textAlignCenter
    %h5
      Total patient:
      = @total_patient_count
  .span8.textAlignCenter
    %h5
      Total Test Done:
      = @total_patient_count
.row
  .span8.textAlignCenter
